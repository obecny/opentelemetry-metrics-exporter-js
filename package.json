{
  "name": "@lightstep/opentelemetry-exporter-metric-js",
  "version": "0.7.0",
  "description": "OpenTelemetry Metric Exporter JS.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/lightstep/opentelemetry-metrics-exporter-js.git"
  },
  "scripts": {
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "compile": "tsc -p .",
    "lint": "gts check",
    "lint:fix": "gts fix",
    "precompile": "tsc --version",
    "prepare": "npm run compile",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "example": "node example/node.js",
    "test": "nyc ts-mocha -p tsconfig.json test/**/*.test.ts",
    "watch": "tsc -w"
  },
  "keywords": [
    "lightstep",
    "metrics",
    "opentelemetry",
    "nodejs",
    "stats",
    "plugin",
    "tracing"
  ],
  "author": "OpenTelemetry Authors",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@opentelemetry/exporter-prometheus": "^0.7.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.6.8",
    "@types/sinon": "^9.0.0",
    "@types/webpack-env": "1.13.9",
    "babel-loader": "^8.0.6",
    "codecov": "^3.6.1",
    "gts": "^1.1.2",
    "mock-require": "^3.0.3",
    "mocha": "^6.2.3",
    "nyc": "^15.0.1",
    "request": "^2.88.2",
    "sinon": "^9.0.2",
    "ts-loader": "^6.2.2",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.10.1",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "3.7.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@opentelemetry/api": "^0.7.0",
    "@opentelemetry/base": "^0.7.0",
    "@opentelemetry/core": "^0.7.0",
    "@opentelemetry/metrics": "^0.7.0",
    "url-parse": "^1.4.7"
  }
}
